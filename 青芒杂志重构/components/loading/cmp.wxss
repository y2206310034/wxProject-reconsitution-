/* components/loading/cmp.wxss */

.container{
    position: relative;
    width:60rpx;
    height:60rpx;
}

.circle{
    position: absolute;
    width:100%;
    height:100%;
    background-color:#333;
    opacity:0.6;
    border-radius: 50%;
    animation: run 2s infinite;
}
.circle1{
    animation-delay: -1s;
}
@keyframes run{
    0%{
        transform: scale(0);
    }
    50%{
        transform: scale(1);
    }
    100%{
        transform: scale(0);
    }
}